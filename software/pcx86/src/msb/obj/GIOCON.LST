 0000                           CONDSP: 
 0000  0000 E                   	DW	(DERBFM)	;test EOF for file opened to this device 
 0002  0000 E                   	DW	(DERBFM)	;LOC 
 0004  0000 E                   	DW	(DERBFM)	;LOF 
 0006  0028 R                   	DW	(_RET)		;perform special CLOSE functions for this device 
 0008  0000 E                   	DW	(DERBFM)	;set device width 
 000A  0000 E                   	DW	(DERBFM)	;GET/PUT random record from/to this device 
 000C  001E R                   	DW	(CONOPN)	;perform special OPEN functions for this device 
 000E  0000 E                   	DW	(DERBFM)	;input 1 byte from file opened on this device 
 0010  002C R                   	DW	(CONSOT)	;output 1 byte to file opened on this device 
 0012  0026 R                   	DW	(CONGPS)	;POS 
 0014  0029 R                   	DW	(CONGWD)	;get device width 
 0016  0028 R                   	DW	(_RET)		;set device comma width 
 0018  0028 R                   	DW	(_RET)		;get device comma width 
 001A  0000 E                   	DW	(DEVBIN)	;block input from file opened on this device 
 001C  0000 E                   	DW	(DEVBOT)	;block output to file opened on this device 
                                 
                                ;CONOPN - perform any device dependent open functions. 
                                ; Entry - [AL]=  device id 
                                ;                0 if default device, 
                                ;                1..n for Disk A:, B:, ... 
                                ;                -1..-n for non-disk devices 
                                ;         [BX] = file number (0..n) 
                                ;         [CX] = random record size if [FILMOD] = random 
                                ;                (if [CX] = 0, use default record size) 
                                ;         [DI] = device offset (2=SCND, 4=SCRN, etc.) 
                                ;         [FILMOD] = file mode 
                                ;                    MD.SQI = 1 ;sequential input 
                                ;                    MD.SQO = 2 ;sequential output 
                                ;                    MD.RND = 3 ;random 
                                ;                    MD.APP = 4 ;append 
                                ;         [FILNAM] = filename 
                                ;         [FILEXT] = 1..3 byte filename extension 
                                ; Exit  - [SI] points to new FDB 
                                ;         FDB is linked into FDB chain with all standard 
                                ;         fields initialized. 
                                ;         All other registers are preserved. 
                                ; 
                                 
                                	EXTRN	INIFDB:NEAR 
                                 
 001E                           CONOPN: 
 001E  B4 02                    	MOV	AH,LOW OFFSET MD_SQO	;allow open for output only 
 0020  BA 00FF                  	MOV	DX,255D		;initial file column position=0 
                                				;initial file width=255 
 0023  E9 0000 E                	JMP	INIFDB 
                                 
