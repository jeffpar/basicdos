{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "BASIC-DOS",
      "url": "http://localhost:4040/demos/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "BASIC-DOS with Dual Monitors",
      "url": "http://localhost:4040/demos/dual/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "BASIC-DOS with Dual Monitors and Sessions",
      "url": "http://localhost:4040/demos/dual/multi/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "BASIC-DOS Build Machine",
      "url": "http://localhost:4040/build/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "BASIC-DOS Build Machine with Dual Monitors",
      "url": "http://localhost:4040/build/dual/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "gulp: watch",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
      "args": ["watch"],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "console": "internalConsole"
    }
  ]
}
